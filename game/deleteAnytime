<canvas id="elementCanvas" width="1600" height="700"> </canvas>
<script>
function findDistanceBetweenPoints(x1,y1,x2,y2) {
	console.log(x1,y1)
	console.log(x2,y2)
	var top = (y2 - y1);
	console.log(top)
	var topSquared = (top*top); 
	console.log(topSquared)
	var bottom =  (x2 - x1);
	console.log(bottom)
	var bottomSquared = (bottom*bottom);
	console.log(bottomSquared)
	var addSqFrac = (topSquared+bottomSquared);
	console.log(addSqFrac)
	var distance = Math.sqrt(parseInt(addSqFrac,10));
	console.log(distance)
};

findDistanceBetweenPoints(2,3,5,9);

</script>
</html>
